{
  "$schema": "https://turbo.build/schema.json",
  "globalDependencies": ["**/.env.*local"],
  "pipeline": {
    "build": {
      "dependsOn": ["^build"],
      "outputs": [".next/**", "!.next/cache/**"]
    },
    "dev": {
      "cache": false,
      "persistent": true
    },
    "dev:docs": {
      "cache": false,
      "persistent": true
    },
    "prepare": {
      "dependsOn": ["^prepare"],
      "outputs": [".nuxt"]
    }
  }
}
